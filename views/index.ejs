<div class="author">
    <h1 class="author__header">Hi, I am ...</h1>
    <p class="author__body">Description of the bloger</p>
</div>

<img src="/img/" alt="description of the image" class="image" width="" height="">

<section class="articles">
    <h2 class=""articles__heading>Latest Posts</h2>

    <ul class="article-ul">

        <% data.forEach(post => { %>

        <li>
            <a href="/post/<%= post.id %>">
                <span><%= post.titel %></span>
                <span class="article-list__date"><%= post.createdAd.toDateString() %></span>
            </a>
        </li>

        <% }) %>
       
    </ul>

    <% if(nextPage !== null){ %>
        <a href="/?page=<% nextPage %>" class="pagination">&lt; View Older Posts</a>
    <% } %>

</section>